        "@types/node": "^14.14.31",
        "autocannon": "^7.0.5",
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-prom-bundle": "^6.3.3",
        "nodemon": "^2.0.7",
        "prom-client": "^13.1.0",
        "typescript": "^4.1.5"
